{
  "name": "@ovnigames/framework",
  "version": "0.3.1",
  "description": "Front-side components of the OVNIGames framework",
  "author": "kylekatarnls",
  "license": "MIT",
  "keywords": [
    "Angular",
    "Library"
  ],
  "scripts": {
    "ng": "ng",
    "start-dev": "ng serve --open --proxy-config=proxy.dev.conf.js --port=1137",
    "start-prod": "ng serve --open --proxy-config=proxy.prod.conf.js --port=1137",
    "start-local": "ng serve --open --proxy-config=proxy.conf.js --port=1138",
    "build": "ng build",
    "build-lib": "ng build OvniGames && copy .\\LICENSE .\\dist\\ovni-games && copy .\\README.md .\\dist\\ovni-games",
    "npm-pack": "cd dist/ovni-games && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publish-current": "node publish.js",
    "test": "ng test OvniGames",
    "lint": "ng lint OvniGames",
    "e2e": "ng e2e OvniGames",
    "build-doc": "./node_modules/.bin/compodoc -p projects/ovni-games/tsconfig.doc.json -n @ovnigames --disablePrivate",
    "run-doc": "node doc.js",
    "sync-version": "node sync-version.js && git add projects/ovni-games/package.json && git commit -m \"Synchronize version\"",
    "push-version": "git push --follow-tags && npm run sync-version && npm run package && npm run publish-current",
    "patch-release": "npm version patch && npm run push-version",
    "minor-release": "npm version minor && npm run push-version",
    "major-release": "npm version major && npm run push-version"
  },
  "dependencies": {
    "@angular/animations": "^8.0.1",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.1",
    "@angular/compiler": "^8.0.1",
    "@angular/core": "^8.0.1",
    "@angular/forms": "^8.0.1",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.1",
    "@angular/platform-browser-dynamic": "^8.0.1",
    "@angular/router": "^8.0.1",
    "apollo-angular": "^1.6.0",
    "apollo-angular-link-http": "^1.8.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-link": "^1.2.12",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.9",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.1",
    "matches-selector-polyfill": "^1.0.0",
    "rxjs": "^6.5.2",
    "socket.io-client": "^2.2.0",
    "url-polyfill": "^1.1.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.3",
    "@angular-devkit/build-ng-packagr": "^0.800.3",
    "@angular/cli": "^8.0.3",
    "@angular/compiler-cli": "^8.0.1",
    "@angular/language-service": "^8.0.1",
    "@compodoc/compodoc": "^1.1.9",
    "@types/graphql": "^14.2.1",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "@types/socket.io-client": "^1.4.32",
    "codelyzer": "^4.5.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.5.1",
    "glob": "^7.1.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.7.1",
    "opn": "^5.5.0",
    "protractor": "^5.4.2",
    "semver": "^6.1.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.34.3",
    "tslib": "^1.10.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
