{
  "name": "@ovnigames/js-dev-tools",
  "version": "0.0.1",
  "description": "Development tools for front-size components for the OVNIGames framework",
  "author": "kylekatarnls",
  "license": "MIT",
  "main": "dist/out-tsc/index.js",
  "types": "dist/out-tsc/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config=proxy.prod.conf.js --port=1138",
    "start-local": "ng serve --open --proxy-config=proxy.conf.js --port=1138",
    "build": "ng build",
    "build-lib": "ng build NgxMatOvniGames",
    "npm-pack": "cd dist/ngx-mat-ovni-games && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "./node_modules/.bin/compodoc -p projects/ngx-mat-ovni-games/tsconfig.doc.json -n @ovnigames --disablePrivate",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "minor-release": "npm version minor && npm publish && git push --follow-tags",
    "major-release": "npm version major && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "@types/socket.io-client": "^1.4.32",
    "apollo-angular": "^1.5.0",
    "apollo-angular-link-http": "^1.4.0",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-client": "^2.4.7",
    "apollo-link": "^1.2.4",
    "core-js": "^2.5.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.0",
    "rxjs": "^6.3.3",
    "socket.io-client": "^2.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.7",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "^7.0.7",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@compodoc/compodoc": "^1.1.7",
    "@types/jasmine": "^2.8.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "^3.1.6"
  }
}
